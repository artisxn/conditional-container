{
    "name": "codicastudio/conditional-container",
    "description": "A noobs worthless garbage, just learning.",
    "keywords": [
        "worthless garabage by a noob."
    ],
    "authors": [
        {
            "name": "Michael J Brown"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4.0"
    },
    "autoload": {
        "psr-4": {
            "codicastudio\\ConditionalContainer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
